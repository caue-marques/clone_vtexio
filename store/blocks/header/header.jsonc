{
  "header": {
    "blocks": [
      "header-layout.desktop",
      "header-layout.mobile"
    ]
  },
  "header.full": {
    "blocks": [
      "header-layout.desktop",
      "header-layout.mobile"
    ]
  },
  "header-layout.desktop": {
    "children": [
      "flex-layout.row#1-desktop",
      "flex-layout.row#2-desktop",
      "header-row#4-desktop"
    ]
  },
  "flex-layout.row#1-desktop": {
    "children": [
      "telemarketing"
    ],
    "props": {
      "fullWidth": true
    }
  },
  "flex-layout.row#2-desktop": {
    "children": [
      "flex-layout.row#barhome"
    ],
    "props": {
      "fullWidth": "true"
    }
  },
  "flex-layout.row#barhome": {
    "children": [
      "rich-text#produtos-personalizados",
      "rich-text#vezes-juros"
    ],
    "props": {
      "blockClass": "home-notification-bar"
    }
  },
  "rich-text#produtos-personalizados": {
    "props": {
      "text": "\nPRODUTOS PERSONALIZADOS",
      "blockClass": "produtos-personalizados"
    }
  },
  "rich-text#vezes-juros": {
    "props": {
      "text": "\n EM ATÃ‰ 3X SEM JUROS",
      "blockClass": "vezes-juros"
    }
  },
  "flex-layout.col#spacer": {
    "props": {
      "width": "grow"
    }
  },
  "header-row#4-desktop": {
    "props": {
      "blockClass": "sticky-header"
    },
    "children": [
      "flex-layout.row#4-desktop"
    ]
  },
  "flex-layout.row#4-desktop": {
    "props": {
      "blockClass": "main-header",
      "horizontalAlign": "center",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true
    },
    "children": [
      "flex-layout.col#logo-desktop",
      "flex-layout.col#category-menu",
      "logo#searchIconDesktop",
      "login",
      "minicart.v2"
    ]
  },
  "flex-layout.col#logo-desktop": {
    "props": {
      "verticalAlign": "middle"
    },
    "children": [
      "logo#desktop"
    ]
  },
  "flex-layout.col#category-menu": {
    "props": {
      "verticalAlign": "middle"
    },
    "children": [
      "flex-layout.row#menu-desktop"
    ]
  },
  "flex-layout.row#menu-desktop": {
    "children": [
      "flex-layout.col#col1-desktop",
      "flex-layout.col#col2-desktop",
      "flex-layout.col#col3-desktop",
      "flex-layout.col#col4-desktop",
      "flex-layout.col#col5-desktop",
      "flex-layout.col#col6-desktop",
      "flex-layout.col#col7-desktop"
